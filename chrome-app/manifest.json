{
  "manifest_version": 2,
  "name": "LegendaryDB",
  "description": "Track your progress in building a legendary weapon in Guild Wars 2 (GW2).",
  "version": "1.0.0",
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "icons": {
    "16": "16.png",
    "128": "128.png"
  },
  "offline_enabled": true,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
